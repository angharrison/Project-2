<div class="card">
  <div class="card-content">
    <a class="card-title" href="/tweet/{{id}}">
      {{ content }}
    </a>
  </div>
  <div class="card-action">
    <a href="/user/{{ character.id }}" class="">{{ character.local }}</a>
    <span class="scent__date">{{ createdAt }}</span>
    <span class="scent_comments">{{ comments.length }} Comments</span>
    <form class="" action="/scent/{{id}}?_method=DELETE" method="POST">
      <button type="submit" name="button">Delete</button>
    </form>
  </div>
</div>
